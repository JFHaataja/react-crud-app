{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "plugin:jsdoc/recommended",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins":
      ["react", "jsdoc"],
  "rules": {
    "indent": "off",
    "require-jsdoc": "off",
    "jsdoc/require-jsdoc": "off"
  }
}
